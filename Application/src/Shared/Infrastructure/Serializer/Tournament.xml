<?xml version="1.0" ?>
<serializer xmlns="http://symfony.com/schema/dic/serializer-mapping"
            xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
            xsi:schemaLocation="http://symfony.com/schema/dic/serializer-mapping
        https://symfony.com/schema/dic/serializer-mapping/serializer-mapping-1.0.xsd">
    <class name="InnovatikLabs\Game\Tournament\Domain\Model\Tournament">
        <attribute name="id" serialized-name="id" >
            <group>readable</group>
        </attribute>
        <attribute name="name" serialized-name="name">
            <group>readable</group>
        </attribute>
        <attribute name="fromDate" serialized-name="fromDate">
            <group>readable</group>
        </attribute>
        <attribute name="toDate" serialized-name="toDate">
            <group>readable</group>
        </attribute>
        <attribute name="logo" serialized-name="logo">
            <group>readable</group>
        </attribute>
    </class>
</serializer>
